{
  "manifest_version": 3,
  "name": "Open Dictionary",
  "version": "1.0.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["contextMenus", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "icons": {
    "16": "favicon-16x16.png",
    "32": "favicon-32x32.png"
  }
}
